{
  "entrypoint": "index.html",
  "shell": "src/tracking-app.html",
  "fragments": [
    "src/my-view1.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "data/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "manifest.json"
  ],
  "lint": {
